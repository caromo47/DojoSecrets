<form action="/users" method="post">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
	Name:<input type="text" id="Name" name = "user[name]">
	Email:<input type="text" id="Email" name = "user[email]">
	Password:<input type="password" id="Password" name = "user[password]">
	Password Confirm:<input type="password" id="Password_Confirmation" name = "user[password_confirmation">
	<input type="submit" name="Join" value="Join">
</form>
